{
  "name": "satellite-searcher",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "security": "npm run security:audit && npm run security:snyk",
    "security:audit": "npm audit --audit-level=moderate",
    "security:snyk": "snyk test",
    "security:retire": "retire --outputformat json",
    "security:outdated": "npm outdated",
    "lint:security": "eslint . --config .eslintrc.js --no-eslintrc --env node --rule 'no-eval: error' --rule 'no-implied-eval: error'"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "escape-html": "^1.0.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "eslint": "^8.0.0",
    "eslint-plugin-security": "^1.0.0",
    "snyk": "^1.0.0"
  }
}
